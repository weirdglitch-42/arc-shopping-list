{
  "compilerOptions": {
    "target": "ES2020",
    "module": "None",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "classic",
    "declaration": false,
    "sourceMap": true,
    "removeComments": false,
    "checkJs": true,
    "allowJs": true,
    "noEmit": false
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "release",
    "scripts",
    "main.js",
    "app.js"
  ]
}
